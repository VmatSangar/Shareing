/**
 *
 * @author toshiba
 */
import java.util.*;

public class LatihanLoopingSederhana {
    
    public static void main(String[] args){
        
        System.out.println("Ini adalah awal program");
        
        //Program untuk menjumlahkan angka dengan rentang
        
        int nilaiAwal,nilaiAkhir,total;
        
        Scanner inputUser = new Scanner (System.in);
        System.out.print("Masukan Nilai Awal = ");
        nilaiAwal = inputUser.nextInt();
        System.out.print("Masukan Nilai Akhir = ");
        nilaiAkhir = inputUser.nextInt();
        
        total = 0;
        
        while(nilaiAwal <= nilaiAkhir){
            total = total + nilaiAwal;
            System.out.println("Ditambah " + nilaiAwal + "Menjadi" + total);
            nilaiAwal++;
        }
        
        do { 
            total = total + nilaiAwal;
            System.out.println("Ditambah " + nilaiAwal + "Menjadi" + total);
            nilaiAwal++;
            }while(nilaiAwal <= nilaiAkhir);
        
        for(;nilaiAwal <= nilaiAkhir; nilaiAwal++){
            total = total + nilaiAwal;
            System.out.println("Ditambah " + nilaiAwal + " Menjadi " + total);
        }  
    }
}

/**
 *
 * @author toshiba
 */
import java.util.*;

public class LatihanIfBersarang{
    
    public static void main(String[]args){
        
      int a,b,c;
     
      Scanner userInput = new Scanner (System.in);
      System.out.println("Ini adalah awal program");
      
      System.out.print("Masukan Nilai A = ");
      a = userInput.nextInt();
      System.out.print("Masukan Nilai B = ");
      b = userInput.nextInt();
      System.out.print("Masukan Nilai C = ");
      c = userInput.nextInt();
      
      if (a == 10){//ekspresi 1
          System.out.println("ini adalah statment 1");
      
        }else if (b <50){//ekspresi 2
          System.out.println("ini adalah statment 4");
        
        }else if (c == 100){//ekspresi 3
          System.out.println("ini adalah statment 2");
        
        }else{ 
        System.out.println("ini adalah statment salah 3");
        }    
   }          
}

/**
 *
 * @author toshiba
 */
import java.util.*;

public class LatihanIfBersarang{
    
