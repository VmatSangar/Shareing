      System.out.print("Masukan Nilai B = ");
      b = userInput.nextInt();
      System.out.print("Masukan Nilai C = ");
      c = userInput.nextInt();
      
      if (a == 10){//ekspresi 1
          System.out.println("ini adalah statment 1");
      
        }else if (b <50){//ekspresi 2
          System.out.println("ini adalah statment 4");
        
        }else if (c == 100){//ekspresi 3
          System.out.println("ini adalah statment 2");
        
        }else{ 
        System.out.println("ini adalah statment salah 3");
        }    
   }          
}

/**
 *
 * @author toshiba
